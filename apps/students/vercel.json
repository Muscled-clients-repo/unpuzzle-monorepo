{
  "buildCommand": "cd ../.. && pnpm build --filter=unpuzzle-student",
  "installCommand": "cd ../.. && pnpm install",
  "rewrites": [
    {
      "source": "/checkout/(.*)",
      "destination": "/checkout/[id]"
    }
  ],
  "functions": {
    "app/checkout/[id]/page.tsx": {
      "maxDuration": 10
    }
  }
}