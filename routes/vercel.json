{
  "rewrites": [
    {
      "source": "/instructor/:match*",
      "destination": "https://instructor.nazmulcodes.org/:match*"
    },
    {
      "source": "/sign-up",
      "destination": "https://core.nazmulcodes.org/sign-up"
    },
    {
      "source": "/sign-in",
      "destination": "https://core.nazmulcodes.org/sign-in"
    },
    {
      "source": "/api/:match*",
      "destination": "https://core.nazmulcodes.org/api/:match*"
    },
    {
      "source": "/course-video/:id",
      "destination": "https://core.nazmulcodes.org/course-video/:id"
    },
    {
      "source": "/:path*",
      "destination": "https://student.nazmulcodes.org/:path*"
    }
  ]
}
